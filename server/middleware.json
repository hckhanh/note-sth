{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    },
    "helmet#xssFilter": {},
    "helmet#frameguard": {
      "params": {
        "action": "deny"
      }
    },
    "helmet#contentSecurityPolicy": {
      "params": {
        "directives": {
          "defaultSrc": [
            "'self'"
          ],
          "styleSrc": [
            "'self'",
            "'unsafe-inline'"
          ],
          "fontSrc": [
            "*.sharepointonline.com"
          ]
        }
      }
    },
    "helmet#hsts": {
      "params": {
        "maxAge": 63072000,
        "includeSubdomains": true,
        "preload": true
      }
    },
    "helmet#hidePoweredBy": {},
    "helmet#ieNoOpen": {},
    "helmet#noSniff": {},
    "helmet#noCache": {}
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": {}
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "./middleware/error": {},
    "strong-error-handler": {}
  }
}
