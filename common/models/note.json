{
  "name": "Note",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "any",
      "required": true
    },
    "created": {
      "type": "date",
      "required": false,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
